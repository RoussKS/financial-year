{
    "name": "roussks/financial-year",
    "description": "A simple library that accepts start of financial year date and splits into financial periods for business purposes. Accepts different interfaces to work with various time handling libraries",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "RoussKS",
            "homepage": "https://www.roussks.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "RoussKS\\FinancialYear\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RoussKS\\FinancialYear\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.8",
        "phpunit/phpunit": "^8"
    }
}